<?xml version='1.0'?>
<!--
Naming Rules:

Category Names:
veg     - ID example V1
dairy   - ID example D1
meat    - ID example M1
bakery  - ID example B1
seafood - ID example S1
frozen  - ID example F1

Unit Names:
lb
each

Types:
e.g. Mexico;Malaysia;Peru
Note that there is no space in between two categories but divided by ;

-->
<products>
    <!-- Fruit and Vegetables -->
    <product category="veg" id="V1">
        <name>Banana</name>
        <price>1.29</price>
        <unit>lb</unit>
        <description>Healthy fruit</description>
        <types>Mexico;Malaysia;Peru</types>
        <img>null</img>
        <stock>512</stock>
    </product>

    <!-- Dairy Products -->
    <product category="dairy" id="D1">
        <name>Milk - 1L</name>
        <price>1.90</price>
        <unit>each</unit>
        <description>Milk</description>
        <types>0%;2%;3.5%</types>
        <img>null</img>
        <stock>200</stock>
    </product>

    <!-- Meat and Poultry -->
    <product category="meat" id="M1">
        <name>Lean Ground Beef</name>
        <price>13.21</price>
        <unit>each</unit>
        <description>Healthy meat</description>
        <types>normal;</types>
        <img>null</img>
        <stock>512</stock>
    </product>

    <!-- Bakery -->
    <product category="bakery" id="B1">
        <name>French Baguette</name>
        <price>3.19</price>
        <unit>each</unit>
        <description>Classic</description>
        <types>Normal;</types>
        <img>null</img>
        <stock>512</stock>
    </product>

    <!-- Sea Food -->
    <product category="seafood" id="S1">
        <name>Lobster Cooked</name>
        <price>31.29</price>
        <unit>each</unit>
        <description>Healthy</description>
        <types>Boston;</types>
        <img>null</img>
        <stock>512</stock>
    </product>

    <!-- Froze -->
    <product category="frozen" id="F1">
        <name>Frozen Pizza</name>
        <price>5.6</price>
        <unit>each</unit>
        <description>Healthy food</description>
        <types>Napoli;New York;Chicago</types>
        <img>null</img>
        <stock>512</stock>
    </product>

</products>
