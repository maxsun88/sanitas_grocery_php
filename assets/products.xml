<?xml version='1.0'?>
<!--
Naming Rules:

Category Names:
veg     - ID example V01
dairy   - ID example D01
meat    - ID example M01
bakery  - ID example B01
seafood - ID example S01
frozen  - ID example F01

Unit Names:
lb
each

Types:
e.g. Mexico;Malaysia;Peru
Note that there is no space in between two categories but divided by ;

-->

<products>
    <!-- Fruit and Vegetables -->
    <product category="veg" id="V01">
        <name>Banana</name>
        <price>1.29</price>
        <unit>lb</unit>
        <description>Healthy fruit</description>
        <types>Mexico;Malaysia;Peru</types>
        <category>veg</category>
        <img></img>
        <stock>512</stock>
    </product>

    <!-- Dairy Products -->
    <product category="dairy" id="D01">
        <name>Milk - 1L</name>
        <price>1.90</price>
        <unit>each</unit>
        <description>Milk</description>
        <types>0%;2%;3.5%</types>
        <category>dairy</category>
        <img></img>
        <stock>200</stock>
    </product>

    <!-- Meat and Poultry -->
    <product category="meat" id="M01">
        <name>Lean Ground Beef</name>
        <price>13.21</price>
        <unit>each</unit>
        <description>Healthy meat</description>
        <types>normal;</types>
        <category>meat</category>
        <img></img>
        <stock>512</stock>
    </product>

    <!-- Bakery -->
    <product category="bakery" id="B01">
        <name>French Baguette</name>
        <price>3.19</price>
        <unit>each</unit>
        <description>Classic</description>
        <types>Normal;</types>
        <category>bakery</category>
        <img></img>
        <stock>512</stock>
    </product>

    <!-- Sea Food -->
    <product category="seafood" id="S01">
        <name>Lobster Cooked</name>
        <price>31.29</price>
        <unit>each</unit>
        <description>Healthy</description>
        <types>Boston;</types>
        <category>seafood</category>
        <img></img>
        <stock>512</stock>
    </product>

    <!-- Froze -->
    <product category="frozen" id="F01">
        <name>Frozen Pizza</name>
        <price>5.6</price>
        <unit>each</unit>
        <description>Healthy food</description>
        <types>Napoli;New York;Chicago</types>
        <category>frozen</category>
        <img></img>
        <stock>512</stock>
    </product>

</products>
